{
  "framework": "nextjs",
  "regions": ["lhr1"],
  "redirects": [
    {
      "source": "/locksmith",
      "destination": "https://emergency-locksmith.vercel.app/",
      "permanent": false
    },
    {
      "source": "/locksmith/:path*",
      "destination": "https://emergency-locksmith.vercel.app/:path*",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/handyman", "destination": "https://handyman-liard.vercel.app/handyman" },
    { "source": "/handyman/(.*)", "destination": "https://handyman-liard.vercel.app/handyman/$1" }
  ]
}
